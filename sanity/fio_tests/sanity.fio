[sanity]
do_verify=1
verify=md5
direct=1
rwmixread=0
group_reporting=1
rw=${RW}
runtime=${RUNTIME}
filename=${FILENAME}
ioengine=${IOENGINE}
iodepth=${IO_DEPTH}
numjobs=${NUM_JOBS}
bs=${BLOCK_SIZE}
size=${SIZE}
ioengine=${IOENGINE}
